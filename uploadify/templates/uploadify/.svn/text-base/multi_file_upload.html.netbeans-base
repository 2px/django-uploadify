{% load site_tags %}<script type="text/javascript" src="{% media_url %}js/uploadify/swfobject.js"></script>
<script type="text/javascript" src="{% media_url %}js/uploadify/jquery.uploadify.v2.1.0.min.js"></script>
<div id="uploadify" class="multi-file-upload"><input id="fileInput" name="fileInput" type="file" /></div>
<script type="text/javascript">// <![CDATA[
$(document).ready(function() {
$('#fileInput').uploadify({
'uploader'  : '{% media_url %}js/uploadify/uploadify.swf',
'script'    : '{% url uploadify_upload %}',
'cancelImg' : '{% media_url %}js/uploadify/cancel.png',
'auto'      : true,
'folder'    : '{% media_url %}uploads/',
'multi'     : true,
'onAllComplete' : allComplete
});
});

function allComplete(event, data) {
    $('#uploadify').load('{{ upload_complete_url }}', {
        'filesUploaded': data.filesUploaded,
        'errorCount': data.errors,
        'allBytesLoaded': data.allBytesLoaded,
        'speed': data.speed
    });
}
// ]]></script>